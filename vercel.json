{
  "functions": {
    "api/**/*.ts": {
      "memory": 256,
      "maxDuration": 10
    },
    "api\\data\\oprs.ts": {
      "memory": 256,
      "maxDuration": 10
    },
    "api\\data\\myor.ts": {
      "memory": 256,
      "maxDuration": 10
    },
    "api\\data\\interbank_rates.ts": {
      "memory": 256,
      "maxDuration": 10
    },
    "api\\data\\interbank_volumes.ts": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/pages/index.tsx"
    }
  ]
}